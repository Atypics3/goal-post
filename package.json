		"express-async-handler": "^1.2.0",
		"express-rate-limit": "^7.4.0",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.5.1",
		"react-scripts": "^5.0.1"
	},
	"devDependencies": {
		"nodemon": "^3.1.4"
	}
